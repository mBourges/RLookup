{
  "name": "react-lookup",
  "version": "0.0.0-semantically-released",
  "description": "autocomplete for react support sync & promise",
  "main": "dist/bundle.js",
  "scripts": {
    "prebuild": "rm -rf dist && mkdir dist",
    "build": "webpack",
    "commit": "git-cz",
    "check-coverage": "istanbul check-coverage --statements 0 --branches 0 --functions 0 --lines 0",
    "test": "mocha  -R spec --compilers js:babel-register src/index.test.js",
    "test:single": "istanbul cover -x *.test.js _mocha -- -R spec src/index.test.js --compilers js:babel-register",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mBourges/react-lookup.git"
  },
  "keywords": [
    "react",
    "autocomplete",
    "typeahead"
  ],
  "author": "Matthieu Bourges <m.bourges@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mBourges/react-lookup/issues"
  },
  "homepage": "https://github.com/mBourges/react-lookup#readme",
  "dependencies": {
    "immutable": "3.7.6",
    "react": "0.14.7",
    "react-dom": "0.14.7",
    "rx": "4.0.7"
  },
  "devDependencies": {
    "babel-core": "6.4.5",
    "babel-loader": "6.2.1",
    "babel-preset-react": "6.3.13",
    "babel-preset-es2015": "^6.3.13",
    "babel-register": "6.4.3",
    "chai": "3.5.0",
    "commitizen": "2.5.0",
    "core-js": "2.0.3",
    "cz-conventional-changelog": "1.1.5",
    "expect": "1.13.4",
    "expect-jsx": "2.2.2",
    "ghooks": "1.0.3",
    "istanbul": "0.4.2",
    "mocha": "2.4.5",
    "react-addons-test-utils": "0.14.7",
    "semantic-release": "4.3.5",
    "webpack": "1.12.12"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "pre-commit": "npm run test"
    }
  }
}
