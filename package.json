{
  "name": "react-lookup",
  "version": "0.0.0-semantically-released",
  "description": "autocomplete for react support sync & promise",
  "main": "src/index.js",
  "scripts": {
    "commit": "git-cz",
    "test": "mocha src/index.test.js -w",
    "test:single": "mocha src/index.test.js",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mBourges/react-lookup.git"
  },
  "keywords": [
    "react",
    "autocomplete",
    "typeahead"
  ],
  "author": "Matthieu Bourges <m.bourges@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mBourges/react-lookup/issues"
  },
  "homepage": "https://github.com/mBourges/react-lookup#readme",
  "dependencies": {
    "immutable": "3.7.6",
    "react": "0.14.7",
    "react-dom": "0.14.7",
    "rx": "4.0.7"
  },
  "devDependencies": {
    "chai": "3.5.0",
    "commitizen": "2.5.0",
    "cz-conventional-changelog": "1.1.5",
    "ghooks": "1.0.3",
    "mocha": "2.4.5",
    "semantic-release": "^4.3.5"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "pre-commit": "npm run test:single"
    }
  }
}
